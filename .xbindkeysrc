"xbacklight -inc 10"
    XF86MonBrightnessUp
"xbacklight -dec 10"
    XF86MonBrightnessDown

"amixer -q sset Master 10%+"
    XF86AudioRaiseVolume
"amixer -q sset Master 10%-"
    XF86AudioLowerVolume
"amixer -q sset Master toggle"
    XF86AudioMute

"spectacle -m"
    Print
"spectacle -r"
    control + Print
"spectacle -a"
    shift + Print

"~/.xmonad/nm.sh"
    control + alt + shift + m

"qdbus org.freedesktop.ScreenSaver /ScreenSaver Lock"
    control + shift + l
