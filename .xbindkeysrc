"xbacklight -inc 10"
    XF86MonBrightnessUp
"xbacklight -dec 10"
    XF86MonBrightnessDown

"amixer -q sset Master 10%+"
    XF86AudioRaiseVolume
"amixer -q sset Master 10%-"
    XF86AudioLowerVolume
"amixer -q sset Master toggle"
    XF86AudioMute
"gnome-screenshot"
    Print


"~/.xmonad/nm.sh"
    control + alt + shift + m
