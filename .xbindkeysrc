"tomahawk --playpause"
    XF86AudioPlay
"tomahawk --next"
    XF86AudioNext
"tomahawk --prev"
    XF86AudioPrev
"tomahawk --stop"
    XF86AudioStop

"xbacklight -inc 5"
    XF86MonBrightnessUp
"xbacklight -dec 5"
    XF86MonBrightnessDown

"amixer -q sset Master 10%+"
    XF86AudioRaiseVolume
"amixer -q sset Master 10%-"
    XF86AudioLowerVolume
"sakura -x lyvi"
    control + alt + m
"amixer -q sset Master toggle"
    XF86AudioMute
"amixer -q sset Master toggle"
    alt + m
"amixer -q sset Speaker unmute"
    alt + k
"amixer -q sset Speaker mute"
    alt + l
"scrot -e 'mv $f ~/screenshots/'"
    Print
