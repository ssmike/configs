"xbacklight -inc 10"
    XF86MonBrightnessUp
"xbacklight -dec 10"
    XF86MonBrightnessDown

"amixer -q sset Master 10%+"
    XF86AudioRaiseVolume
"amixer -q sset Master 10%-"
    XF86AudioLowerVolume
"amixer -q sset Master toggle"
    XF86AudioMute
"gnome-screenshot"
    Print

"amixer -q sset Master toggle"
    alt + l
"amixer -q sset Speaker unmute"
    alt + k
"amixer -q sset Speaker mute"
    alt + m

"~/nm.sh"
    control + alt + shift + m
