"xbacklight -inc 10"
    XF86MonBrightnessUp
"xbacklight -dec 10"
    XF86MonBrightnessDown

"amixer -q sset Master 10%+"
    XF86AudioRaiseVolume
"amixer -q sset Master 10%-"
    XF86AudioLowerVolume
"amixer -q sset Master toggle"
    XF86AudioMute

"shutter --full"
    Print
"shutter --select"
    control + Print
"shutter --window"
    shift + Print
"shutter --active"
    alt + Print

"~/.xmonad/nm.sh"
    control + alt + shift + m

"qdbus org.freedesktop.ScreenSaver /ScreenSaver Lock"
    control + shift + l
